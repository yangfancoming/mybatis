<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.apache.ibatis.submitted.nonexistentvariables.Mapper">
	<select id="count" resultType="int">
		select count(*) from names where id=#{thisvariabledoesnotexist} and firstName=#{name}
	</select>
</mapper>
